{
  "manifest_version": 2,

  "name": "Healthy Habits",
  "description": "Develop and monitor a more productive schedule!",
  "version": "1.0",
  "author": "Beth McNany",
  "homepage_url":"http://www.cs.umd.edu/~beth",
  
  "permissions": [
    "history", "tabs", "http://*/*", "https://*/*"
  ],
  
  "background": {
    "scripts": ["jquery-1.11.0.min.js", "background.js"]
  },
  
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
  
  "browser_action": {
	"default_title": "View Schedule",
    "default_icon": {
		"19": "icon19.png",
		"38": "icon38.png"
	}
  }
}
