{
  "manifest_version": 2,

  "name": "818G URL logger",
  "description": "Chrome extension to log browsing history",
  "version": "1.0",
  "author": "Beth McNany",
  "homepage_url":"http://www.cs.umd.edu/~beth",
  
  "permissions": [
    "history"
  ],
  
  "background": {
    "scripts": ["background.js"]
  },
  
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
